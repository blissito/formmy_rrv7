{
  "name": "formmy-sdk",
  "version": "1.0.4",
  "description": "RAG as a Service - Official TypeScript SDK for Formmy. Upload documents, query knowledge base with AI-powered semantic search.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./llamaindex": {
      "import": "./dist/llamaindex/index.js",
      "types": "./dist/llamaindex/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./errors": {
      "import": "./dist/errors.js",
      "types": "./dist/errors.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "formmy",
    "rag",
    "rag-as-a-service",
    "vector-database",
    "semantic-search",
    "llamaindex",
    "document-intelligence",
    "knowledge-base",
    "embeddings",
    "mongodb",
    "ai",
    "llm",
    "chatbot",
    "pdf-parser",
    "typescript",
    "sdk",
    "nodejs"
  ],
  "author": {
    "name": "Formmy",
    "email": "support@formmy.app",
    "url": "https://formmy.app"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blissito/formmy_rrv7.git",
    "directory": "sdk/formmy-parser"
  },
  "homepage": "https://formmy.app",
  "bugs": {
    "url": "https://github.com/blissito/formmy_rrv7/issues",
    "email": "support@formmy.app"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "llamaindex": ">=0.3.0",
    "zod": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "llamaindex": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "validate": "node validate-package.js",
    "preversion": "npm run validate"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "pdf-parse": "^2.4.5"
  }
}
