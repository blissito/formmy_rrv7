  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "dev": "react-router dev",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "start:original": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:production": "npm run build && cross-env NODE_ENV=production node ./server.js",
    "test:e2e": "playwright test --headed",
    "test:e2e:headless": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:inject-session": "tsx tests/e2e/inject-session.ts",
    "test:e2e:setup-auth": "playwright test tests/e2e/setup-auth.spec.ts --headed",
    "test:e2e:smoke": "playwright test tests/e2e/00-smoke.spec.ts --headed",
    "test:e2e:auth": "playwright test tests/e2e/01-auth.spec.ts --headed",
    "test:e2e:chatbot": "playwright test tests/e2e/02-chatbot.spec.ts --headed",
    "test:e2e:rag": "playwright test tests/e2e/03-rag.spec.ts --headed",
    "test:e2e:apikeys": "playwright test tests/e2e/04-apikeys.spec.ts --headed",
    "test:e2e:ghosty": "playwright test tests/e2e/05-ghosty.spec.ts --headed",
    "test:e2e:report": "playwright show-report",
    "typecheck": "react-router typegen && tsc",
    "postinstall": "prisma generate",
    "dev:setup": "npx playwright install chromium && prisma generate",
    "playwright:install": "npx playwright install chromium",
    "deploy": "fly deploy --now",
    "deploy:force": "fly deploy --no-cache --now",
    "audit:memory": "npx tsx scripts/audit-conversation-memory.ts",
    "voice:dev": "tsx server/voice/livekit-agent-worker.ts dev",
    "voice:start": "tsx server/voice/livekit-agent-worker.ts start"
  },
