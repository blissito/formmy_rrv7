{
  "name": "formmy",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "dev": "react-router dev",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "start:original": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:production": "npm run build && cross-env NODE_ENV=production node ./server.js",
    "typecheck": "react-router typegen && tsc",
    "postinstall": "prisma generate",
    "dev:setup": "npx playwright install chromium && prisma generate",
    "playwright:install": "npx playwright install chromium",
    "deploy": "./scripts/fast-deploy.sh",
    "deploy:force": "fly deploy --no-cache"
  },
